  <script>
    function getSubCatData()
    {
      let CatId = document.getElementById('categoryId').value;
      console.log(CatId)
      $.ajax({
        url:"/admin/ExtraCaregory/getSubCatData",
        method:"POST",
        data:{
          CatId : CatId
        },
        success: async (res)=>{
          $('#subcategoryId').html(res);
        }
      })
    }

    function getExtraCatData()
    {
      let CatId = document.getElementById('categoryId').value;
      let SubCatId = document.getElementById('subcategoryId').value;
      $.ajax({
        url:"/admin/Brand/getExtraCatData",
        method:"POST",
        data:{
          CatId : CatId,
          SubCatId : SubCatId,
        },
        success: async (res)=>{
          $('#extracategoryId').html(res);
        }
      })
    }

    function getBrandType()
    {
      let CatId = document.getElementById('categoryId').value;
      let SubCatId = document.getElementById('subcategoryId').value;
      let ExtraCatId = document.getElementById('extracategoryId').value;
      $.ajax({
        url:"/admin/ExtraCaregory/getBrandType",
        method:"POST",
        data:{
          CatId : CatId,
          SubCatId : SubCatId,
          ExtraCatId :ExtraCatId
        },
        success: async (res)=>{
          $('#Brandtype').html(res);
        }
      })
    }


    function removeOneImg(path,index,ProId)
    {
      $.ajax({
        url:"/admin/Product/removeOneImg",
        method:"POST",
        data:{
          ProductId : ProId,
          path : path,
          index:index,
        },
        success: async (res)=>{
          $(`#ProductImg${ProId}`).html(res);
        }
      })
    }
    
    
  </script>
